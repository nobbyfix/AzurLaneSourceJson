      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "あとそっちの！……ちょっと！天灯（てんとう）と花火に触れるな！ああもう！まだ始めていいって言ってないわよ！……"
