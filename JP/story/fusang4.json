        "x": 15,
        "number": 2
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_main_day"
    },
    {
      "actor": 305010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "きゃっ！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 30,
          "type": "shake",
