      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
      "say": "かなりリアルな造形だから、こんな暗い場所で見たらそりゃ驚くよね……はは。",
      "actor": 203030,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
