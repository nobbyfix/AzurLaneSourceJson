      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 1
    },
    {
      "say": "その時はジャンヌ、聖堂の奪還を優先しましょう",
      "actor": 805010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
