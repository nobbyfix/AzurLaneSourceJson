    {
      "actor": 307060,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
