    },
    {
      "actor": 305020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "殿様？外は雨ですよ？？",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:79}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_room"
    },
    {
      "actor": 305020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "……え、ええと、そのまま後ろを向いてくだされれば大、大丈夫ですぅ……",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:79}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_room"
    },
    {
      "actor": 305020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
