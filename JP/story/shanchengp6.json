      "actorName": "{namecode:79}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_room"
    },
    {
      "actor": 305020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "ええと、お着替えお着替え……ありました！早速――",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:79}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_room"
    },
    {
      "actor": 305020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "options": [
        {
          "content": "無言で背を向ける",
          "flag": 1
        },
        {
          "content": "外に行く",
          "flag": 2
        }
      ],
      "say": "うぅ…殿様もいますね……ご、ごめんなさい……",
