      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 206040,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "そうですね…私、ほとんどの先輩たちと耳の形が違うので、自分でもちょっと不思議です。",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 207020,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "確かに重桜の獣耳とも違うな。なんでだろう。",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "paintingFadeOut": {
        "time": 0.5,
        "side": 0
