      "paintingFadeOut": {
        "time": 0.5,
        "side": 2
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "say": "えっと、私にもよくはわかりませんが、時々なんとなくこんな考えが浮かぶことがあります……",
      "actor": 206040,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
