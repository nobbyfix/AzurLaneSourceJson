    },
    {
      "actor": 206040,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "ヴィクトリアス先輩も！おはようございます！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "effects": [
        {
