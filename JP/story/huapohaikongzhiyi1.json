    },
    {
      "actor": 107230,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "いまの私たちの実力であれば、完全に打って出られるわ！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_map_maliyana"
    },
    {
      "actor": 102140,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "姉貴、ぼくも打って出たほうがいいと思います！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
