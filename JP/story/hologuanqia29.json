      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "あっちに掘って行けば、確かあの例のお墓みたいな場所まで行っちゃうよね…",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_holosaba1"
