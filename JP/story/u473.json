        [
          "やさしくて静かであたたかくて\n\n<size=45>三 プレゼント</size>",
          1
        ]
      ],
      "stopbgm": true
    },
    {
      "actor": 408020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
