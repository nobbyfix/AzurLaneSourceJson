    },
    {
      "actor": 405010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "任せて頂戴。枢機卿殿が心配していることなら承知しているわ",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_guild_blue"
    },
    {
      "say": "エウロパ大陸の航路――それは私たちがお互い信頼しあってこそ維持できるわ",
      "actor": 405010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
