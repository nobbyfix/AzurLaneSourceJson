      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "ビスマルクさん、くれぐれも技術供与協定の内容の確認を頼みます",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
