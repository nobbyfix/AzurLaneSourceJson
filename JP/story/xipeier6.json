      "actorName": "プリンツ・オイゲン",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 403010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "う……うるさいっての！だいたいどうして、あんたがそれを知ってるのっ！？隠れて見てたとか悪趣味だっての！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "アドミラル・ヒッパー",
      "painting": {
        "alpha": 0.3,
        "time": 1
