      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 2
    },
    {
      "actor": 900298,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "actorShadow": true,
      "say": "「赤城は元気にしていますか？あの子のことをあなたに頼みましたけど、ちゃんと面倒を見れていますか？」",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "天城？",
      "painting": {
        "alpha": 0.3,
        "time": 1
