    },
    {
      "actor": 900012,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "blackBg": true,
      "say": "あの台詞……余程の自信がないと吐けないわよね",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "オブザーバー",
      "painting": {
