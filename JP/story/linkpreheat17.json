    },
    {
      "actor": 900011,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "blackBg": true,
      "say": "まさか利用したはずの世界に、逆用されてしまうだなんて……",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "テスター",
      "painting": {
        "alpha": 0.3,
        "time": 1
      }
    },
    {
      "actor": 900021,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "blackBg": true,
      "say": "そこまで深刻でもないんじゃない？こんなの「ちょっかいをかけられた」程度でしょ？",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
