    {
      "say": "……まさか僕にもこんな日が来るとはね",
      "actor": 105141,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
