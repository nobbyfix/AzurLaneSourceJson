      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
      "say": "まだ決めていないよ。ただでさえ助っ人がおろそかになってたし、しばらく時間を置いてから考えても良さそうだ",
      "actor": 103160,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
