    },
    {
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "options": [
        {
          "content": "ここは――",
          "flag": 1
        }
      ],
      "say": "同志指揮官はそろそろ起きたかな？",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "？？？",
      "painting": {
        "alpha": 0.3,
        "time": 1
