    },
    {
      "mode": 1,
      "sequence": [
        [
          "<size=51>「焔の海で身を焼かれようと、その歩みは止まることを知らず」</size>",
          1
        ],
        [
          "<size=51>「受難の物語は終わり、希望への道筋が開かれる」</size>",
          3
        ],
        [
          "<size=51>「ふふ、しかし……」</size>",
          5
        ],
        [
          "<size=51>「それを享受できるものは限られた存在だと……」</size>",
          7
        ],
        [
          "<size=51>「貴方は知っているのだろうか…？」</size>",
          9
        ]
