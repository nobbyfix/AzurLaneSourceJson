    },
    {
      "actor": 403030,
      "side": 2,
      "dir": 1,
      "paintingNoise": false,
      "say": "ええ、バッチリ聞こえてるわ。これからはその通信機を使ってちょうだい",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "nameColor": "#a9f548",
      "painting": {
        "alpha": 0.3,
        "time": 1
