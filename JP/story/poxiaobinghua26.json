    },
    {
      "actor": 900136,
      "side": 2,
      "dir": 1,
      "paintingNoise": true,
      "nameColor": "#ffff4d",
      "say": "「最後の光がかき消されるまで、頑張って戦ってください」",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "記録機構",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_cccpv2_7"
    },
    {
      "say": "！！「光の都市」が、少しずつ暗くなっていく…！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "effects": [
