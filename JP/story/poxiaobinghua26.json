      "bgName": "bg_cccpv2_7"
    },
    {
      "actor": 900136,
      "side": 2,
      "dir": 1,
      "paintingNoise": true,
      "nameColor": "#ffff4d",
      "say": "「ありません」",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "記録機構",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_cccpv2_7"
    },
    {
      "say": "………………",
