      "actor": 900011,
      "side": 1,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "actorName": "テスター",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "そんな兵器ごときが、まさか「ヒト」の自由やら正義やら論じるなんて"
    },
