    {
      "actor": 303110,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "むっ、それもそうなのだが……",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
