      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.5,
        "dur": 0.5
      },
      "say": "ひとまず予定通り、最初は我が陣営の演習の様子をご覧いただけただろうか",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "flashout": {
        "alpha": [
          0,
          1
