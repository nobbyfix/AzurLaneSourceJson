      "actor": 205030,
      "side": 2,
      "dir": 1,
      "nameColor": "#ffff4d",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "わかったわ。……あなたにアイリスの加護があらんことを"
