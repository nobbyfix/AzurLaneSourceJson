      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_fuxiangxian_2"
    },
    {
      "say": "今回は違うようね…！",
      "actor": 307060,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
