      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_italyv2_3"
    },
    {
      "actor": 605010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
      "say": "（いつもはリットリオのことをちょっとせっかちかなーって思っていましたけど、今日の作戦は完全にこちらの失敗ですね）",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
