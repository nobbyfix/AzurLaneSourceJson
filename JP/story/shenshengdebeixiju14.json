        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_italy"
    },
    {
      "actor": 605020,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "expression": 5,
      "say": "競泳水着とビギニ、どっちのほうがいいかな？",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_italy"
    },
    {
      "actor": 900198,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "expression": 1,
      "say": "んー最近はビギニのほうが流行っていますね…",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_italy"
    },
    {
