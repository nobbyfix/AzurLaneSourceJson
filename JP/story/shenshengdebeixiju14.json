      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_italy"
    },
    {
      "actor": 900198,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "expression": 3,
      "say": "（みんな無事とはいえ、うん……色々困っちゃいますね……）",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_italy"
    },
    {
