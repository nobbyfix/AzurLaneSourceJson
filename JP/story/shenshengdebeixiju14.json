      "actor": 605020,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "expression": 6,
      "say": "ふん、私は最初からそう考えていたわよ",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_italy"
    },
