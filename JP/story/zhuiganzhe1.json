  "scripts": [
    {
      "mode": 1,
      "sequence": [
        [
          "お嬢様とめぐるひとときを\n\n<size=45>一 青空と始まりの音</size>",
          1
        ]
      ],
      "stopbgm": true
    },
    {
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "painting": {
