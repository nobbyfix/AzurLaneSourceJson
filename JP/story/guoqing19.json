    {
      "actor": 307020,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff0000",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "それがこの「聖域」で実験をする理由だ"
    },
    {
      "actor": 307020,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff0000",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "いつかわかるはずだ。私たちのやったことの意味、理由を……"
