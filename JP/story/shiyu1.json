  "scripts": [
    {
      "mode": 1,
      "sequence": [
        [
          "世話焼き娘はダテじゃない\n\n<size=45>一 嵐の起こった翌日に</size>",
          1
        ]
      ],
      "stopbgm": true
    },
    {
      "side": 2,
      "dir": 1,
      "flashout": {
        "alpha": [
          0,
          1
        ],
        "dur": 1,
        "black": true
      },
      "painting": {
        "alpha": 0.3,
