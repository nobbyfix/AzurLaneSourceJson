  "scripts": [
    {
      "mode": 1,
      "sequence": [
        [
          "笑顔なあの子の胸のウチ\n\n<size=45>二 姉と妹と指揮官と</size>",
          1
        ]
      ],
      "stopbgm": true
    },
    {
      "side": 2,
      "dir": 1,
      "flashout": {
        "alpha": [
          0,
