    },
    {
      "actor": 403040,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "！？みんな、それを知ってて……",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
