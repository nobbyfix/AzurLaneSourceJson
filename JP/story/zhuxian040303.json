      "nameColor": "#ff5c5c",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "そしたら情報交換も無理、ということね…今は敵とはいえ…"
