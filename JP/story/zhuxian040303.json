      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "私たち、龍驤ちゃんとそっくりのニセモノに出くわして、今倒したところ"
