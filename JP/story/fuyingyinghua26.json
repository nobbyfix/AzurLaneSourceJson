      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "あ、長門様、【ワタツミ】の儀式のリハーサル準備できたよ！"
    },
    {
      "actor": 305050,
      "side": 2,
