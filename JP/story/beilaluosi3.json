    },
    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "確かに、秘書艦に任命した最初の日は驚かされたな。",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_107"
    },
    {
      "side": 2,
