  "scripts": [
    {
      "actor": 905010,
      "side": 1,
      "say": "よくここまで来たな！侵略者ども！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "story-french1",
      "painting": {
        "alpha": 0.3,
