      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
      "actor": 10300030,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "し、指揮官さま？あの、これはちょっと……わたし、本を探してて……",
