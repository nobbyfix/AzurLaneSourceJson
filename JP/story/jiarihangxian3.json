      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_106"
    },
    {
      "say": "こんにちは。わたしはみさきの姉の、なぎさです。いきなりでよくわからないけど……よろしくお願いします。",
      "actor": 900304,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
