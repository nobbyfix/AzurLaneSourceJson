      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "withoutPainting": true,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "イストワール",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
