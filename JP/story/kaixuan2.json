      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "秘書艦に注意された……",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "say": "参加者一覧にざっと目を通し、いくつか指摘した。",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "side": 2,
      "dir": 1,
      "flashout": {
        "alpha": [
          0,
