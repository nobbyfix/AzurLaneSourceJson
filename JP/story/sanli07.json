    },
    {
      "actor": 301130,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "偉そうな人…うぅ……ど、どういうこと？",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:15}",
      "painting": {
