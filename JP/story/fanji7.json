    },
    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "その後も二人は数々の動きで高得点を見せ、本日の主役といってもいいほどの結果を残したのだった。",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_nepu1"
    },
    {
      "actor": 204020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "指揮官ー！！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
