    {
      "actor": 202120,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "致し方ありませんね。……陛下、今日は新しいお仲間が着任するとお聞きしましたが。"
    },
    {
      "actor": 205010,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Zzzz……新しい仲間……本当！？私の知り合いなの！？……頭……重い……ベル……起こして……"
    },
    {
      "actor": 202120,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
