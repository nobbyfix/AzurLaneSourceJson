    },
    {
      "actor": 301100,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 8,
      "say": "Aaaah! Ryuuhou, watch out!!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 30,
          "type": "shake",
          "dur": 0.15,
