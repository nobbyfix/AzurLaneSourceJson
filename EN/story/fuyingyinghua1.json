    },
    {
      "actor": 306070,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "After years of suffering... No, we've been training for this very moment, and it is time to show the entire world our might!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_xinnong_cg1"
    },
