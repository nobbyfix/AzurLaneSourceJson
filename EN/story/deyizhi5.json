        {
          "pos": {
            "x": 1500
          },
          "actor": 403040
        }
      ],
      "hideOther": true,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Thank you for coming, Eugen, Spee, Tirpitz, and Z1. I have something to tell you.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
