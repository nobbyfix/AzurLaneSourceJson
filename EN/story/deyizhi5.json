      "nameColor": "#a9f548",
      "subActors": [
        {
          "pos": {
            "x": 1500
          },
          "actor": 403040
        }
      ],
      "hideOther": true,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Indeed. It's also caused trouble for other people. Furthermore, Deutschland knows about it now, and she feels really hurt by it.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
