      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "subActors": [
        {
          "pos": {
            "x": 1500
          },
          "actor": 403040
        }
      ],
      "hideOther": true,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "I know it's for her sake, but it's a fact that what we've done isn't right.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
