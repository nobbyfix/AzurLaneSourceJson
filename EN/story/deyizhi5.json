      "nameColor": "#a9f548",
      "subActors": [
        {
          "pos": {
            "x": 1500
          },
          "actor": 403040
        }
      ],
      "hideOther": true,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "That is why I've called you here - so we can discuss what to do next.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
