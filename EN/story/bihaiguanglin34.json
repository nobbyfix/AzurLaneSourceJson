      "bgm": "battle-xinnong-image",
      "nameColor": "#a9f548",
      "say": "Somewhere in the Sakura Empire...",
      "flashin": {
        "dur": 1,
        "black": true,
        "alpha": [
          1,
          0
        ],
        "delay": 1
      },
      "painting": {
