    },
    {
      "actor": 408040,
      "side": 0,
      "dir": 1,
      "nameColor": "#ffde38",
      "blackBg": true,
      "expression": 2,
      "say": "Hey, um! Your performance yesterday was absolutely fantastic, and I will never ever in my whole life forget it!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "action": [
