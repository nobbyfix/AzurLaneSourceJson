      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_119"
    },
    {
      "actor": 202112,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
      "say": "Here you are! And thanks so much for your help, Smalley.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_119"
    },
    {
      "say": "It was nothing. Thank the Halloween Delivery Service.",
      "actor": 101421,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_119"
    },
    {
      "actor": 301411,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Hip~ Hop~ Trick or treat. Mikazuki craves taiyaki.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 30,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
          "delay": 0,
          "x": 0
        }
      ],
      "painting": {
