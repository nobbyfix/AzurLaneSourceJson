      "actor": 900209,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Yeah, we already knew that! Nagato's selling point is her gap moe!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 30,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
          "delay": 0,
          "x": 0
        }
      ],
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_111"
    },
    {
      "say": "Since we're all here, let's play with Nagato~!",
      "actor": 900209,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_111"
    },
    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "The girls' joyful voices resounded from the building protected by the towering walls of the castle.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_111"
    },
    {
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "side": 2,
      "dir": 1,
      "typewriter": {
