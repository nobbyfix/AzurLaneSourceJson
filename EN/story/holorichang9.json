      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 1,
        "dur": 0.5
      },
      "say": "My sincerest apologies for my reaction earlier. I must offer my apologies as the Priestess of the Sakura Empire.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "flashout": {
        "alpha": [
          0,
          1
        ],
        "dur": 0.5,
        "black": true
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_111"
    },
    {
      "say": "Lady Nagato, you don't need to...",
      "actor": 301490,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_111"
    },
    {
      "actor": 900215,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "She no longer seems like she's being put on a pedestal. If anything, she's way more approachable this way.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_111"
    },
    {
      "actor": 900210,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "She looks just like a cute little girl from the neighborhood~",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_111"
    },
    {
      "actor": 900212,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
      "say": "Hnnnggghhhh...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_111"
    },
    {
