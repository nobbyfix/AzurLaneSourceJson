      "nameColor": "#ffff4d",
      "oldPhoto": true,
      "options": [
        {
          "content": "I don’t know...",
          "flag": 1
        }
      ],
      "say": "Didn’t we agree that you’d be heading to Samos with the others? Why are you here all by yourself?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "？？",
      "painting": {
        "alpha": 0.3,
        "time": 1
