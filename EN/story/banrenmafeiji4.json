    {
      "actor": 201212,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Rodney! Dunkerque! Émile! Over here!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
