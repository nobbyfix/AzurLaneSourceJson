      "sequence": [
        [
          "Heart in the Right Place\n\n<size=45>1 The Day After the Storm</size>",
          1
        ]
      ],
      "stopbgm": true
    },
    {
      "side": 2,
      "dir": 1,
      "flashout": {
        "alpha": [
          0,
          1
        ],
        "dur": 1,
        "black": true
      },
      "painting": {
        "alpha": 0.3,
