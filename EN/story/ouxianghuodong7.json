      },
      "bgName": "star_level_bg_120"
    },
    {
      "actor": 101020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Yeah...! When I see Lexington cheering alongside us, it's as if energy keeps welling up from within me!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 30,
          "type": "shake",
          "dur": 0.2,
          "number": 2,
