      },
      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.5,
        "dur": 0.5
      },
      "say": "Mikasa! The enemy known as \"Ember\" managed to escape from us... and she's headed towards --",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "xinnong-4",
      "bgmDelay": 1,
      "nameColor": "#a9f548"
    },
    {
      "actor": 305110,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "What?!"
    },
    {
      "say": "——",
      "soundeffect": "event:/battle/boom2",
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "dialogShake": {
        "speed": 0.09,
        "x": 8.5,
