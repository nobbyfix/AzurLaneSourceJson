      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.5,
        "dur": 0.5
      },
      "say": "The port's newest recruits bring not only their skills to the table, but a smile to everyone's face. And so, the Venus girls' vacation continues.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
