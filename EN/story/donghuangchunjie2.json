    },
    {
      "actor": 501041,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "Instead of making firecrackers, how about we just play...",
      "typewriter": {
        "speed": 0.05,
