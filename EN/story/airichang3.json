      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_103"
    },
    {
      "say": "Yeeaaahhh! If I blow up then I'll get the Commander to put up a stage for me to use in the dorm! That's my goal!",
      "actor": 102080,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_103"
    },
    {
