    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 307050,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "actorName": "{namecode:95}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
