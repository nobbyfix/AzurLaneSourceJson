    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 306051,
      "side": 0,
      "dir": -1,
      "nameColor": "#a9f548",
      "actorName": "{namecode:87}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
