    {
      "actor": 107060,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Yep~ {namecode:96}, the tempura you made are truly amazing."
