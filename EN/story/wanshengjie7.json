    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 301171,
      "side": 0,
      "dir": -1,
      "nameColor": "#a9f548",
      "actorName": "{namecode:19}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
