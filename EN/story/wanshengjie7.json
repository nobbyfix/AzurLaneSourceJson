    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 307060,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "actorName": "{namecode:96}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "I made a lot of tempura, everyone feel free to enjoy them~"
