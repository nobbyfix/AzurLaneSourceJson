      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
      "say": "Gascogne, why did you get in our way?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "action": [
