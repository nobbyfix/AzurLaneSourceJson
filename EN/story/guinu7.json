      "mode": 1
    },
    {
      "actor": 302080,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "flashout": {
        "alpha": [
          0,
