      "effects": [
        {
          "name": "speed",
          "active": true
        }
      ],
      "say": "G-give it your all, D-Denver!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
