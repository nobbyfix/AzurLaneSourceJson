      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_nepu1"
    },
    {
      "say": "I'll suppress them with my own cannon fire! I-58, circle around to attack them with torpedoes!",
      "actor": 103060,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
