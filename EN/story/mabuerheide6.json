    {
      "dir": 1,
      "painting": {
        "time": 1,
        "alpha": 0.3
      },
      "actor": 102270,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "This is me. Used to be me, anyway. I was a lame, nerdy girl who stuffed her head with useless knowledge, like that pink noise stuff. Kinda just slipped out of me.",
      "bgName": "bg_main_twilight",
      "typewriter": {
        "speedUp": 0.01,
        "speed": 0.05
      },
      "side": 2
    },
    {
      "dir": 1,
      "painting": {
        "time": 1,
        "alpha": 0.3
      },
      "actor": 102270,
      "nameColor": "#a9f548",
      "expression": 7,
      "say": "Anyway, I'm happy with the way I am now. I don't wanna go back to the old me, not at all.",
      "bgName": "bg_main_twilight",
      "typewriter": {
        "speedUp": 0.01,
        "speed": 0.05
      },
      "side": 2
    },
    {
      "dir": 1,
      "painting": {
        "time": 1,
        "alpha": 0.3
      },
      "actor": 102270,
      "nameColor": "#a9f548",
