    {
      "dir": 1,
      "painting": {
        "time": 1,
        "alpha": 0.3
      },
      "actor": 102270,
      "nameColor": "#a9f548",
      "expression": 5,
      "say": "...My eyes are up here, y'know. Or are you just curious 'bout this photo?",
      "bgName": "bg_main_twilight",
