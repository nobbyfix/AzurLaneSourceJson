      "nameColor": "#a9f548",
      "effects": [
        {
          "name": "speed",
          "active": false
        }
      ],
      "say": "I'd like to extend my gratitude to Hiei and Seattle, whose experience with hosting large events was invaluable to us.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
