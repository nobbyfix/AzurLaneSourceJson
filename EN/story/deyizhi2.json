    {
      "actor": 202120,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Because you have been assigned to the exercise fleet for the time being, Your Majesty.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
