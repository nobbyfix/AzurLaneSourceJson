      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.5,
        "dur": 0.5
      },
      "say": "How interesting, the queen of the Royal Navy is being lectured by one of her own servants! An Iron Blood leader would never be lectured by her servant!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "flashout": {
        "alpha": [
          0,
          1
