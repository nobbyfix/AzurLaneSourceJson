    {
      "actor": 107090,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "Enterprise! Do you need anything?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "say": "Can you show me yesterday's commission logs? There's something I wanted to confirm.",
      "actor": 107060,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 107090,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Give me just a moment. Commander, the log should be in that stack by your hand.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "options": [
        {
          "content": "Pass the papers to Enterprise.",
          "flag": 1
