        "time": 1,
        "alpha": 0.3
      },
      "bgName": "star_level_bg_103",
      "typewriter": {
        "speedUp": 0.01,
        "speed": 0.05
      },
      "side": 2,
      "dir": 1,
      "expression": 7
    },
    {
      "actor": 102270,
      "nameColor": "#a9f548",
