    {
      "typewriter": {
        "speedUp": 0.01,
        "speed": 0.05
      },
      "nameColor": "#a9f548",
      "bgName": "star_level_bg_103",
      "say": "As I was passing by, Marblehead called out to me.",
      "side": 2,
      "dir": 1,
      "painting": {
        "time": 1,
        "alpha": 0.3
      }
    },
    {
      "actor": 102270,
      "nameColor": "#a9f548",
      "say": "...Wait, crap, I ruined my chance to do another \"Guess who\"!",
      "options": [
        {
          "content": "\"No need to.\"",
          "flag": 1
        },
        {
          "content": "\"I don't have to guess anymore.\"",
