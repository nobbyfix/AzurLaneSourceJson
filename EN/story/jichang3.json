      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 701050,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "hidePaintObj": true,
      "painting": {
        "alpha": 0.3,
