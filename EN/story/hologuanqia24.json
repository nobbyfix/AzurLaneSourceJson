    {
      "actor": 307060,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Seems to be our only choice... Hololivers, I'd like you to handle communications and base defense, and we'll explore the–"
    },
