      "bgmDelay": 2,
      "bgName": "bg_story_outdoor"
    },
    {
      "say": "...Oh. Portland's team is back.",
      "actor": 103070,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
