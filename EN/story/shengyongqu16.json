      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_qiongding_8"
    },
    {
      "say": "I heard from a certain friend that the Iron Blood has begun to move.",
      "actor": 902010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_qiongding_8"
