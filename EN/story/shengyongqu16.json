      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_qiongding_8"
    },
    {
      "actor": 901110,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "But don't worry about me. Le Terrible brought me some snacks.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_qiongding_8"
    },
    {
      "actor": 902010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "Really? That's fine, but isn't it more fun when you're with everyone else?",
