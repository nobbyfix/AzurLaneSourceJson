      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_qiongding_8"
    },
    {
      "actor": 901110,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "I wanted to, but... Ahem! I mean, I'm on patrol today and could not join you this time.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
