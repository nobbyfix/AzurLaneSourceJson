      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "Wait, Eugen?! What are you doing here?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
