    {
      "side": 2,
      "dir": 1,
      "actorName": "???",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 900315,
      "nameColor": "#ffa500",
      "hidePaintObj": true,
      "bgName": "bg_luoxuan_5",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "I do not understand the principle behind your power, but... the aftershocks from what I am about to do will likely erase the fleeting changes you've been able to effect in this world.",
      "paintingNoise": true
    },
    {
      "side": 2,
      "expression": 1,
