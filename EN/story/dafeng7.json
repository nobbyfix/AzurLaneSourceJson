    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Taihou pulled me close and buried me in her chest. It seems she already knew.",
      "bgName": "bg_story_room"
    },
    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "She knew the only answer I could give her... was \"yes.\"",
      "blackBg": true,
      "bgName": "bg_story_room"
    }
