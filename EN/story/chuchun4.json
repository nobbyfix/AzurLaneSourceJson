      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 301230,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "You did it! Now just give it all you've got, Hatsuharu! I'll leave now so I won't get in your way! See ya!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
