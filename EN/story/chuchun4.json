      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Now I understand. Hatsuharu wants to cure her social anxiety.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "say": "Is... i-is that... g-good?",
