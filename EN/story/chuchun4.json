      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
          "delay": 0,
          "x": 0
        }
      ],
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
