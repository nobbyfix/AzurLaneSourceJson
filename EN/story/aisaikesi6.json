    {
      "actor": 107090,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "W-why are you here, Enterprise?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
          "delay": 0.2,
          "x": 0
        }
      ],
      "painting": {
