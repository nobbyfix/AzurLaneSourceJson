    },
    {
      "actor": 702030,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 5,
      "say": "I didn't expect all of this to be dwelling just beneath us...",
      "typewriter": {
        "speed": 0.05,
