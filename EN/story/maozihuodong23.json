      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_cccp_7"
    },
    {
      "actor": 705050,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
      "say": "We should have addressed this threat long ago...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "options": [
        {
          "content": "Why didn't you...?",
          "flag": 1
