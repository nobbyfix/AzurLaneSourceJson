      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.7,
        "dur": 0.7
      },
      "say": "The next day...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "story-1",
      "bgmDelay": 1.4,
      "bgName": "bg_story_school"
