      "side": 1,
      "dir": 1,
      "nameColor": "#ff0000",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "She'll be fine. She has you if she needs guidance!"
