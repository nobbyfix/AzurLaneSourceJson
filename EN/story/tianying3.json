      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "In order to help Aquila find her missing \"something,\" I explained the situation to the kids who were helping sort out files in the Data Room.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
