    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Then I guess retreating to a safer area is our only choice."
    },
    {
      "say": "Well look at you, aren't you a real strategizer!",
      "actor": 702020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
