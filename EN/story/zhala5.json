      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "I figured this was a good way to see another side of Zara.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 603020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
