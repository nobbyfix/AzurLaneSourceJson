    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "blackBg": true,
      "say": "Her breathy whisper flooded my brain with pleasure, making every nerve from my neck up cry out in ecstasy."
