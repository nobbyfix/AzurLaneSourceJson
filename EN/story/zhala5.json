    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "It was an eye mask. I didn't know what she was up to, but I decided to go along with her anyway.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "blackBg": true,
      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 1,
        "dur": 1
      },
      "say": "Alright, here we go. Don't move, Commander~",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "Zara",
      "painting": {
        "alpha": 0.3,
        "time": 1
