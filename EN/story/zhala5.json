    },
    {
      "actor": 603020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.5,
        "dur": 0.5
      },
      "say": "Now then, come rest your head right here on my lap. Don't be nervous, just relax.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "flashout": {
        "alpha": [
          0,
