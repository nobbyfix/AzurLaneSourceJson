    },
    {
      "actor": 900136,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "blackBg": true,
      "say": "And if we had, would it have changed yours or your Creator's fates?",
      "typewriter": {
        "speed": 0.05,
