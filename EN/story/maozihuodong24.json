        "speed": 0.05,
        "speedUp": 0.01
      },
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "actorName": "???",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_cccp_7"
    },
    {
