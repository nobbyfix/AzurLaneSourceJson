    {
      "actor": 501020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "How was that? That was a proper New Year's greeting, right?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
