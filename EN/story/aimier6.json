      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "options": [
        {
          "content": "W-whoa, now!",
          "flag": 1
        }
      ],
      "say": "Émile casually grabbed hold of my arm without any hesitation.",
      "bgName": "bg_story_school"
    },
    {
      "say": "What? We're on a date, Commander! We need to act appropriately~",
      "actor": 802010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "side": 2,
      "dir": 1,
      "flashout": {
        "alpha": [
          0,
          1
        ],
