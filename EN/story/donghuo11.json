      "actor": 900072,
      "side": 1,
      "dir": 1,
      "nameColor": "#D6341DFF",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
