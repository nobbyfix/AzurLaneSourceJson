      "bgName": "bg_story_school"
    },
    {
      "actor": 102120,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "withoutActorName": true,
      "say": "Honolulu seemed to relax as she hugged the large teddy bear I'd given her.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "story-2",
