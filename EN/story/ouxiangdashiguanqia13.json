      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 312010,
      "side": 2,
      "dir": 1,
      "paintingNoise": true,
      "say": "I'm not so sure myself, nya...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "nameColor": "#a9f548",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 312010,
      "side": 2,
      "dir": 1,
      "paintingNoise": true,
      "say": "Ever since the concert started, the energy signature has been increasing steadily, nya.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "nameColor": "#a9f548",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "say": "Monitor module output: energy reading is coming from atop the stage.",
      "actor": 905030,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
