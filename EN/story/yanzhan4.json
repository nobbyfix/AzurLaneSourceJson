        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.5,
        "dur": 0.5
      },
      "say": "(I'll step out for some fresh air.)",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "flashout": {
        "alpha": [
          0,
          1
        ],
        "dur": 0.5,
