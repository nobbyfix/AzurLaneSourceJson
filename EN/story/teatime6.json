    },
    {
      "actor": 207020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Slowly but surely, the fallen leaves are beginning to pile up in the courtyard..."
    },
    {
      "say": "After all, autumn is the season of fallen leaves.",
      "actor": 207031,
      "side": 1,
      "dir": -1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "paintingFadeOut": {
        "time": 0.5,
        "side": 0
