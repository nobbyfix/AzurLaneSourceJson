      "nameColor": "#a9f548",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_luoxuan_1"
    },
    {
      "actor": 101450,
      "side": 2,
      "dir": 1,
      "paintingNoise": true,
      "hidePaintObj": true,
      "say": "Yesterday evening, a sea fog suddenly appeared around the base, and then Sakura Empire carriers started bombarding us...",
