        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_luoxuan_1"
    },
    {
      "actor": 102290,
      "side": 2,
      "dir": 1,
      "paintingNoise": true,
      "hidePaintObj": true,
