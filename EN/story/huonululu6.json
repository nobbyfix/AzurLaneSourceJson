      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
      "actor": 102120,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "withoutActorName": true,
      "say": "... I won't be able to have a conversation with her like this.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
