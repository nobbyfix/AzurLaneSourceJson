    },
    {
      "actor": 102120,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "withoutActorName": true,
      "say": "... Honolulu seemed like she couldn't let up and relax.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
      "say": "Hey, that's a pretty cute teddy bear...",
      "actor": 102120,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
      "actor": 102120,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "L-look, I didn't mean it like that ... I was just wondering if Helena might like it! C-c'mon, let's get a move on...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
          "number": 3,
          "delay": 0.2,
          "x": 0
        }
