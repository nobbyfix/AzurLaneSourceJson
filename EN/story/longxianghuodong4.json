    {
      "actor": 107060,
      "side": 0,
      "dir": -1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "So, we've got to work together to get out of this..."
