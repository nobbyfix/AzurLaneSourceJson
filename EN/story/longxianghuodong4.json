    {
      "actor": 107060,
      "side": 0,
      "dir": -1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "We have no choice but to defend this island. We can't let them bomb us freely..."
