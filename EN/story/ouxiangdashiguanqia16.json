        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 10700010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "hidePaintEquip": true,
