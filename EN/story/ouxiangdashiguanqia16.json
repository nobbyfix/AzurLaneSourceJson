      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "I'm so sorry, nya. This is all Akashi's fault, nya. Please forgive me, nya...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
