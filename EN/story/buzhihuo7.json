    {
      "say": "You'll understand if you think about it a little, Nyaa~",
      "actor": 312010,
      "side": 2,
      "dir": -1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
