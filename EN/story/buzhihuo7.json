      "dir": 1,
      "nameColor": "#a9f548",
      "options": [
        {
          "content": "Apologize seriously",
          "flag": 1
        }
      ],
      "say": "... Commander, you shouldn't dwell too much on what that dumb cat said. It's best if I handle things by myself.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "story-2",
      "painting": {
        "alpha": 0.3,
        "time": 1
