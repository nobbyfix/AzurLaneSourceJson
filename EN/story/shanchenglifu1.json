      "paintingFadeOut": {
        "time": 0.5,
        "side": 0
      },
      "say": "That should be obvious... Anyway, you should get down from there before you get your new clothes dirty.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:20}",
      "painting": {
        "alpha": 0.3,
