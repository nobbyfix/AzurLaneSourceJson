        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:20}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
      "actor": 304020,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
