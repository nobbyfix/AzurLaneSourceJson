    {
      "actor": 312012,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "I'm wiped, nya... It's not easy setting up an event for everyone here at port, nya...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:98}",
      "painting": {
        "alpha": 0.3,
