        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:71}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
      "actor": 301180,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
