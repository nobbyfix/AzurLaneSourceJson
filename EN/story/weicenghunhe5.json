    {
      "actor": 103240,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 6,
      "say": "Well, I'm sure she just stopped to smell the roses somewhere. Pretty beautiful place we've got around here.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_guild_blue"
