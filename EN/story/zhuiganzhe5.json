      "sequence": [
        [
          "Moments with the Young Miss\n\n<size=45>5 A Passion for the Exotic</size>",
          1
        ]
      ],
      "stopbgm": true
    },
    {
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "flashout": {
        "alpha": [
          0,
