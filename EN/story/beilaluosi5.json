      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.5,
        "dur": 0.5
      },
      "say": "Putting Belorussiya to bed relieved all the tension in me, both physically and mentally. Out of energy, I blacked out on the spot...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
