    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "As soon as the Sirens had made their appearance, they were promptly turned into little more than wrecks.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_battle_night"
