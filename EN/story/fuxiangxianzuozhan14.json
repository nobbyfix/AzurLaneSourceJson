    {
      "actor": 307060,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Yeah! Remember, we have the superior numbers here. Just pick them apart and capitalize on that advantage!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
