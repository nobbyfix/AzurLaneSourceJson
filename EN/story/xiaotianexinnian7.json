      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
      "actor": 102141,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
