      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
      "say": "Is there anything you're hoping will come true, Helena?",
