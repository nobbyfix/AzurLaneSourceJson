    {
      "actor": 399011,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 6,
      "say": "It's called a <i>hatsumode</i>, the first shrine visit of the year. It's said that the first wish of the year you make will always come true.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "{namecode:152}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
      "actor": 102141,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "New Year's wish, huh. (Whispering) I wonder if it'll really come true...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
