    {
      "say": "Chaser quietly pushed the door open and went through.",
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
