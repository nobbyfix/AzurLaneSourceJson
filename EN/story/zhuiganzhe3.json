      "nameColor": "#a9f548",
      "expression": 1,
      "say": "This is the Vichiya dormitory! Let’s hurry inside.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_main_day"
