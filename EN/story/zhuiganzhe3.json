      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 0.5,
        "dur": 0.5
      },
      "say": "Oh~ The interior design is really quite different from the Royal Navy’s...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "flashout": {
        "alpha": [
          0,
          1
        ],
        "dur": 0.5,
