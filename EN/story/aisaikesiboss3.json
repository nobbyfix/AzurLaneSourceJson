      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "Heheh, unlike you, Hornet, Essex is actually the diligent type~",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    "13": {
      "say": "Can't help it.",
