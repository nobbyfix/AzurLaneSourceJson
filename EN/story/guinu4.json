    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "One day, I was overcome with curiosity over what she might be thinking about, and I glanced a little too long.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "say": "What are you staring at me for, Commander?",
