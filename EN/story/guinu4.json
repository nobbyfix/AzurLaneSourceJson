    },
    {
      "actor": 302080,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
      "say": "Don't be shy, feast your eyes. What you see is quality. It was made to be gazed at and admired.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "options": [
        {
          "content": "\"Quality? You mean...?\"",
          "flag": 1
        },
        {
          "content": "\"All right, I'll admit it. I can't take my eyes offâ€“\"",
