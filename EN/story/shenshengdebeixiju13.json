      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_italy_cg4"
    },
    {
      "say": "Aw.......",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "actorName": "Formidable",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_italy_cg4"
    },
    {
