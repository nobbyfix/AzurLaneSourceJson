      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Phew... Thank goodness these were just mass-produced Pawns... I'd hate to come across one of their leaders."
    },
    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 301050,
      "side": 0,
