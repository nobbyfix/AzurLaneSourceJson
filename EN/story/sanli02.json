      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Attention, detecting a large target to our front... Signal identified as... Tester-α."
    },
    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 307050,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
