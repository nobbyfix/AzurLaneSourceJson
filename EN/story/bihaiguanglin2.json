      "side": 2,
      "dir": 1,
      "bgmDelay": 0.5,
      "bgm": "cw-story"
    },
    {
      "actor": 301290,
      "nameColor": "#a9f548",
      "say": "But then I got distracted by a beautiful bird which flew past me. It led me off course, and now I have no idea where I am!",
      "hidePaintObj": true,
      "bgName": "bg_story_tiancheng7",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "side": 2,
      "dir": 1,
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 3
    },
    {
      "actor": 301290,
      "nameColor": "#a9f548",
      "action": [
        {
          "dur": 0.15,
          "number": 2,
          "type": "shake",
          "y": 45,
          "x": 0,
          "delay": 0
        }
      ],
      "say": "It's quite the embarrassing situation... I wonder how future me is going to explain herself to Katsuragi!",
      "hidePaintObj": true,
      "bgName": "bg_story_tiancheng7",
      "typewriter": {
        "speed": 0.05,
