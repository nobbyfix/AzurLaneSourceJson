        "alpha": [
          0,
          1
        ]
      },
      "nameColor": "#a9f548",
      "say": "Sakura Empire Mainland - Shinano's Estate",
      "flashin": {
        "dur": 1,
        "black": true,
        "alpha": [
          1,
          0
        ],
        "delay": 1
      },
      "painting": {
