      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Explosions tear through the waters as the battle rages on.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_qiongding_6"
    },
    {
      "actor": 801030,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "They're so strong, even though they're just mass-produced ships...!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 30,
          "type": "shake",
          "dur": 0.15,
