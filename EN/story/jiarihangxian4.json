      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_main_day"
    },
    {
      "say": "Mm... It looks like this terminal can only receive signals, with no way to... Nya? A new message came through, nya.",
      "actor": 312010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
