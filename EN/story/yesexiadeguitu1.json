    {
      "actor": 901020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "Huh?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
          "delay": 0,
          "x": 0
        }
      ]
    },
    {
      "say": "Jeez... what are you spacing out again for...",
      "actor": 900236,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 4
    },
    {
      "say": "Kersaint, Fougueux, and the others are all ready, so we're just waiting for Tartu now...",
      "actor": 900236,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 6
    },
    {
      "say": "I've been ready though?",
      "actor": 901020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 3
    },
    {
      "say": "Just tell us then if that's the case... All right, before we go, let's go over the mission one more time!",
      "actor": 900236,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 9
