    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 900011,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "actorName": "Tester",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "... ..."
    },
    {
      "say": "I had Eugen leave because there is no telling what will become of me.",
      "actor": 405010,
      "side": 2,
      "dir": 1,
      "nameColor": "#ffde38",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 3
    },
    {
      "say": "But I now know that it is time to end this farce.",
      "actor": 405010,
      "side": 2,
      "dir": 1,
      "nameColor": "#ffde38",
      "typewriter": {
        "speed": 0.05,
