      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Ahem! Nevermind! In any case, let's all review the results of today's joint exercise...",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_nepu1"
