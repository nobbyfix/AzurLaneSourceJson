        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_nepu1"
    },
    {
      "actor": 801010,
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
