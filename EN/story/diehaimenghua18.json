    {
      "actor": 307080,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "(...This \"enemy\"... Is it not the Sirens?)",
      "typewriter": {
        "speed": 0.05,
