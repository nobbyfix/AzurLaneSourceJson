      },
      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 1,
        "dur": 1
      },
      "say": "Mutsu...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "newyear",
      "painting": {
        "alpha": 0.3,
