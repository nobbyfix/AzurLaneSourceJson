      "nameColor": "#a9f548",
      "expression": 2,
      "dialogShake": {
        "speed": 0.09,
        "x": 10,
        "number": 2
      },
      "say": "...Geh?! Oh, Master?! Is something the matter?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
