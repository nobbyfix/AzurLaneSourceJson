    {
      "actor": 103060,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Well, Commander? I think it's a brilliant idea!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
          "delay": 0,
          "x": 0
        }
      ],
      "painting": {
