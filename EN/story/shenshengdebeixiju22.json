    {
      "actor": 404010,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Those who betray the Iron Blood shall quickly find that we are their judge, jury, and executioner."
    },
    {
      "actor": 404010,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
