    },
    {
      "actor": 407020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "Hmph...! That dumb \"commander\" must have taken advantage of me when I wasn't paying attention! Unforgivable! I'll have to prepare some punishment!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "???",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 407020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "You think so too, right? Hiei!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "???",
      "painting": {
        "alpha": 0.3,
        "time": 1
