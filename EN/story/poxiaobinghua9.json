        "black": true,
        "delay": 1,
        "dur": 1
      },
      "say": "Comrade, welcome to the Northern Parliament’s Arcana Sanctum!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "flashout": {
        "alpha": [
          0,
          1
        ],
        "black": true,
        "dur": 1
