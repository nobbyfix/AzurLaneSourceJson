    {
      "say": "That's not good... I hope the commander stays safe...",
      "actor": 202270,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
