      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
      "say": "On this day, at this time, feeling the advent of spring",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_holosaba2"
    },
    {
