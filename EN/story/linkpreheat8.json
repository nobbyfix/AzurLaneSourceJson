      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "actorName": "测试者",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "哼哼哼，第六感很灵敏嘛……“猫神大人”？"
