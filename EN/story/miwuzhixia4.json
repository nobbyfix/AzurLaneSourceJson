      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_xiangting_3"
    },
    {
      "actor": 900021,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
      "say": "Yuup, it's me! The crowdpleaser, the one and only Purifier! Hello there, my little test subjects~",
