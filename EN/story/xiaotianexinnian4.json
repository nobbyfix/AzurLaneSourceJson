    {
      "actor": 399010,
      "side": 0,
      "dir": -1,
      "nameColor": "#a9f548",
      "say": "Since the snow on your clothes is starting to melt, perhaps we should head for the dormitory soon?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
