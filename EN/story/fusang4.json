    {
      "actor": 305010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Eep!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 30,
          "type": "shake",
          "dur": 0.2,
