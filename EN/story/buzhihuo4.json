    {
      "actor": 312010,
      "side": 2,
      "dir": -1,
      "nameColor": "#a9f548",
      "say": "But it's different this time. It's going to be comprehensive, Nyaa!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
