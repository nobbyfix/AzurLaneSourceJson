      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "How are you aiming so well in the middle of the night?!"
    },
    {
      "say": "Ugh... This has to be some kind of cheat...!",
      "actor": 603020,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff5c5c",
