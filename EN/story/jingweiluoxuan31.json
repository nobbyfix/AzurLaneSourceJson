    },
    {
      "actor": 900315,
      "side": 2,
      "dir": 1,
      "nameColor": "#ffa500",
      "hidePaintObj": true,
      "expression": 2,
      "say": "New Jersey! Boise! Morrison! Don't shoot at me!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "？？？",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_luoxuan_5"
    },
    {
      "actor": 900315,
      "side": 2,
      "dir": 1,
      "nameColor": "#ffa500",
      "hidePaintObj": true,
      "expression": 3,
      "say": "Look, those are the ships we've been searching for!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "？？？",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_luoxuan_5"
    },
    {
      "actor": 105170,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "hidePaintObj": true,
      "expression": 5,
      "say": "Appreciate it, whoever you are!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_luoxuan_5"
    },
