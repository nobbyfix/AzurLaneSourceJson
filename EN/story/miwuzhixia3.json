      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_battle_night"
    },
    {
      "actor": 102050,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 4,
      "say": "Roger!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_battle_night"
    },
    {
      "say": "This is just a hunch of mine, but I doubt the Sirens' plan is as one-dimensional as it looks.",
      "actor": 103160,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_battle_night"
    },
    {
      "actor": 103160,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "Let's proceed cautiously and uncover what they're really doing here.",
