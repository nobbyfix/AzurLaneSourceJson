    {
      "actor": 107110,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "No! Operation Siren is a united offensive against the Sirens! We're supposed to stick together, not divide our forces across vast distances!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
