      "say": "What?! An attack from a new enemy? Report the damages, hurry!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgm": "story-6"
