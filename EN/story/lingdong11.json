      "say": "......"
    },
    {
      "actor": 207040,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "N-no, it's something else... The ocean itself is being sucked up into the sky!"
    },
    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
