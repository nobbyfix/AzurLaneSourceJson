      "dir": 1,
      "nameColor": "#ff0000",
      "actorName": "Purifier",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Ahahaha~! What an interesting choice to create a singularity here!"
    },
    {
      "actor": 205070,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Accursed Sirens, is this thy witchcraft?"
