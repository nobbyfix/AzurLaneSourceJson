        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Target the Siren! Fire at will!"
    },
    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 900021,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff0000",
      "actorName": "Purifier",
      "painting": {
