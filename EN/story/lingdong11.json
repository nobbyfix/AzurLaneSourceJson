    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 900021,
      "side": 2,
      "dir": 1,
      "nameColor": "#ff0000",
      "actorName": "Purifier",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Huh? How exactly am I supposed to create a singularity of this size?"
    },
    {
      "actor": 205070,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "We have an emergency situation. Adjusting strategic operations. Prepare all forces for combat!"
