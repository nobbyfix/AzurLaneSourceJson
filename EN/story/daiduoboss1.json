      "say": "In any case, that is what Her Majesty said...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "level02",
      "bgmDelay": 0.5,
      "bgName": "bg_story_nepu2"
