    },
    {
      "nameColor": "#a9f548",
      "expression": 2,
      "say": "The answer in full is: I'm Marblehead, your peppy right-hand girl slash secretary!",
      "flashout": {
        "dur": 0.5,
        "black": true,
        "alpha": [
          0,
          1
