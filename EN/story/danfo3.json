    {
      "actor": 101110,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Salutations, Commander! And Denver, too!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
