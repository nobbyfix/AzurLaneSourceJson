      "bgName": "star_level_bg_113"
    },
    {
      "actor": 307080,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 7,
      "say": "(Destroyers... going to school together...)",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_113"
    },
    {
      "actor": 307080,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 5,
      "say": "(The children of the Sakura Empire... hand in hand with children of the Royal Navy... I see, this is a dream of Azur Lane, reunited...)",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_113"
