    {
      "actor": 305050,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "What do you plan to do with Watatsumi?!"
    },
    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 900012,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "actorName": "Observer",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Hehehe, who knows..."
    },
    {
      "actor": 301490,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Lady Nagato, watch out...!"
