    },
    {
      "actor": 301100,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "blackBg": true,
      "expression": 1,
      "say": "This is just a rehearsal, Yamashiro! Come on, you're up!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      }
    },
    {
      "painting": {
        "alpha": 0.3,
        "time": 1
