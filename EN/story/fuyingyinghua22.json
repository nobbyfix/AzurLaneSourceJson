      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "effects": [
        {
          "name": "juqingshitou",
          "active": false
        }
      ]
    },
    {
      "actor": 305050,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Cease this ceremony this instant! Step away from that stone!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
