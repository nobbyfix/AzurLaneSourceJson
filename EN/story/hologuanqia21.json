      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "effects": [
        {
          "name": "speed",
          "active": false
        }
      ]
    },
    {
      "say": "\"Yamada Hermione\"?! You cheeky Siren bastard! I'm a battle-hardened combat maid who's played through more hardcore games than you can even imagine! Don't you dare make fun of me!",
      "actor": 10500030,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
