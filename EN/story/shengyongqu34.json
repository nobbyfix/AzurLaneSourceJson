    },
    {
      "actor": 900237,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "What an awful place for a tea party... But, what else is one to do in front of the Royal Navy's forward base?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
