      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "… O my, this taste——!!!"
    },
    {
      "actor": 202110,
      "side": 2,
      "dir": -1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
