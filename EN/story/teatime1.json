      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Sister..."
    },
    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 301111,
      "side": 1,
      "dir": -1,
      "nameColor": "#a9f548",
      "actorName": "{namecode:13}",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "—— She left a note and then headed out!"
    },
    {
      "actor": 207031,
      "side": 0,
