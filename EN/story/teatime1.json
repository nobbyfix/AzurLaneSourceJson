      }
    },
    {
      "actor": 207031,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "Huh, {namecode:11} did not come? She shouldn’t be on a mission today, right?"
    },
    {
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actor": 301121,
      "side": 1,
