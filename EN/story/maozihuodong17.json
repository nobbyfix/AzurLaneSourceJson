      "nameColor": "#a9f548",
      "expression": 3,
      "say": "I'll beat you all to a pulp!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_cccp_7"
    },
    {
      "say": "We'll kick out anything that trespasses upon our home, vermin or Siren...!",
      "actor": 701020,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
