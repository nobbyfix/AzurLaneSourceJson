    {
      "actor": 202120,
      "side": 1,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "In short, you should live the way you always have sister. Leave the hard work of being the perfect maid to me."
