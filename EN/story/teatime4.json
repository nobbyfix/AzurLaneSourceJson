    {
      "actor": 202110,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "I don't want to rely on you forever, Belfast… If only I could be more like you..."
