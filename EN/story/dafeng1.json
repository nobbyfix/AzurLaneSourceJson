      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 307070,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "That's part of a secretary ship's duty, after all.",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Somehow, it seemed like a reasonable explanation at the time.",
      "bgName": "bg_story_task"
