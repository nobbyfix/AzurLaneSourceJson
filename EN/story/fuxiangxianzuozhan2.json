      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Heinrich's rigging, a massive mechanical beast affectionately nicknamed \"Eisen,\" fires a volley with a thundering roar.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_fuxiangxian_1"
    },
    {
      "actor": 403090,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "We did it! High five, Eisen!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
