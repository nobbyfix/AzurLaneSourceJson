    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "Prinz Heinrich, the P-class armored ship, weaves between buoys while firing at training targets. She is having a whale of a time.",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_fuxiangxian_1"
    },
    {
