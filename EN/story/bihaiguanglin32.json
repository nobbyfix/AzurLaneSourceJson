    {
      "bgName": "bg_daofeng_6",
      "painting": {
        "time": 1,
        "alpha": 0.3
      },
      "dir": 1,
      "say": "\"I have no more questions for you. The rest of this all is Eugen's problem now.\"",
      "typewriter": {
        "speedUp": 0.01,
