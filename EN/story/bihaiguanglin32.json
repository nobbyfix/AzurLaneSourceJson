    {
      "bgName": "bg_daofeng_6",
      "painting": {
        "time": 1,
        "alpha": 0.3
      },
      "dir": 1,
      "say": "\"Supposing the war comes to an end, what comes next for the world's shipgirls?\"",
      "typewriter": {
        "speedUp": 0.01,
        "speed": 0.05
      },
      "nameColor": "#ffff4d",
      "actorName": "Mysterious Voice",
      "side": 2
    },
    {
      "bgName": "bg_daofeng_6",
      "painting": {
        "time": 1,
        "alpha": 0.3
      },
      "dir": 1,
      "say": "Huh? That depends on what you mean...",
      "typewriter": {
        "speedUp": 0.01,
        "speed": 0.05
      },
      "nameColor": "#a9f548",
      "actorName": "{namecode:18}",
      "side": 2
