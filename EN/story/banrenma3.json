  "scripts": [
    {
      "mode": 1,
      "sequence": [
        [
          "Centaur and the Port \"Elders\"\n\n<size=45>Chapter 3 - The Meaning of Respect</size>",
          1
        ]
      ],
      "stopbgm": true
    },
    {
      "typewriter": {
