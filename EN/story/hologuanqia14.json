      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "expression": 3
    },
    {
      "say": "Don't worry about the TNT too much, just think of it as an everyday item! We need it to construct buildings, you see.",
      "actor": 10500040,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
