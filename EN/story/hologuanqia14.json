      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "You build houses out of dynamite...? What a peaceful world you come from if explosives make the foundations of your buildings..."
    },
    {
      "actor": 10500040,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
