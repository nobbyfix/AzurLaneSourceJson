      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "It's nice to meet you (again.) I'm Minato Aqua, Hololive's local virtual maid!"
    },
    {
      "actor": 10500030,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
      "say": "But just because I'm maid doesn't mean I'm good at housekeeping, that's a stereotype! Video games, however, you bet I am!!!!!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "action": [
        {
          "y": 30,
          "type": "shake",
          "dur": 0.15,
          "number": 2,
          "delay": 0,
          "x": 0
        }
