        "black": true,
        "delay": 1,
        "dur": 1
      },
      "say": "Hold yer horses, Urakaze~! I'm not done yet!",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "story-1",
      "bgmDelay": 2,
      "bgName": "star_level_bg_104"
