    {
      "actor": 102290,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "hidePaintObj": true,
      "expression": 4,
      "say": "Um, Commander... I'm getting nervous from how much you're staring at me...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
