      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "hidePaintObj": true,
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
    {
      "actor": 102290,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "hidePaintObj": true,
      "expression": 4,
      "say": "Um, if you don't need anything...",
