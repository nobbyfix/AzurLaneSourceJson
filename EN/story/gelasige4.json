      "say": "During break...",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "bgm": "story-1",
      "bgmDelay": 2,
      "bgName": "bg_story_school"
    },
    {
      "say": "Sigh...",
      "actor": 202261,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
