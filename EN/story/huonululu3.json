    {
      "actor": 102120,
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "... But why are YOU here, Commander?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
