    },
    {
      "side": 1,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "The 3 of us sat down and enjoyed some good cake and afternoon tea.",
      "bgName": "star_level_bg_104"
    },
    {
      "actor": 202010,
      "side": 2,
      "dir": 1,
