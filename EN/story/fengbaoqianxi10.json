  "scripts": [
    {
      "say": "Now THAT was a battle! We admit defeat.",
      "actor": 205060,
      "side": 1,
      "bgm": "level",
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
