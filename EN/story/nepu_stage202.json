      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "We've received communication signals on an unregistered frequency. This is..."
    },
    {
      "actor": 0,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Ah... testing, testing. Everyone, can you hear me?",
      "typewriter": {
        "speed": 0.05,
