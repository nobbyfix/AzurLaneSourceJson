    },
    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "And the fleet on patrol that day was... Bismarck's!?",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 408040,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "say": "Commander? What's wrong?",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
