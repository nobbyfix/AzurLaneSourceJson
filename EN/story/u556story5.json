        "speed": 0.05,
        "speedUp": 0.01
      },
      "paintingFadeOut": {
        "time": 0.5,
        "side": 0
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_night"
    },
