      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_task"
    },
    {
      "actor": 408040,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 3,
