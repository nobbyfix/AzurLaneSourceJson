      "flashin": {
        "alpha": [
          1,
          0
        ],
        "black": true,
        "delay": 1,
        "dur": 1
      },
      "say": "无限的宇宙，无穷的次元，无数个世界……观测了那么多……",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
