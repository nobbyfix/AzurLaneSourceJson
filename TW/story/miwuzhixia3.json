      "nameColor": "#a9f548",
      "expression": 2,
      "dialogShake": {
        "speed": 0.09,
        "x": 8.5,
        "number": 2
      },
      "say": "没错，这时候应该无视它继续前………哈啊？！！",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "action": [
        {
          "y": 45,
          "type": "shake",
