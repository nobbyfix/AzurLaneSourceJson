      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_main_twilight"
    },
    {
      "say": "嗯？太大……？炫耀……？",
      "actor": 207030,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_main_twilight"
