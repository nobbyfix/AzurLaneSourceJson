      "action": [
        {
          "y": 0,
          "type": "shake",
          "dur": 0.5,
          "number": 2,
          "delay": 0,
          "x": 25
        }
      ],
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
      "say": "我說妳啊，選誰不好，為什麼偏偏找我推銷妳的妹妹啊？",
      "actor": 403040,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
