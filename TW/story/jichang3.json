      "dir": 1,
      "nameColor": "#a9f548",
      "hidePaintObj": true,
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "ええ、アイドルとして、歌で愛と平和を皆に届けるチャンスですわ"
    },
    {
      "actor": 207110,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "hidePaintObj": true,
      "expression": 2,
