        "speed": 0.05,
        "speedUp": 0.01
      },
      "side": 0,
      "dir": 1,
      "nameColor": "#a9f548",
      "actorName": "贝尔法斯特",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_hms_1"
    },
