    },
    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "追赶者微笑着转过身来。",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_main_twilight"
    },
