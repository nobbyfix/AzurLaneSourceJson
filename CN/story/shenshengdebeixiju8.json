      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "女人的直觉"
    },
    {
      "actor": 205029,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 6,
      "say": "女…女人的直觉吗？！？",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "actorName": "厌战",
      "painting": {
        "alpha": 0.3,
        "time": 1
