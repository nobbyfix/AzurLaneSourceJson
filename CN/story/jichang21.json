      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "expression": 1,
      "say": "和上次一样，似乎有不少人都想尝试一下。",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
