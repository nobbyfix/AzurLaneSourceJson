      "nameColor": "#a9f548",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_1100"
    },
    {
      "actor": 107030,
      "side": 2,
      "dir": 1,
      "paintingNoise": true,
      "say": "心中萦绕着这个念头，我加快了搜索步调。",
