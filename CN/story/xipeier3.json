      "actor": 403010,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "action": [
        {
          "y": 30,
          "type": "shake",
          "dur": 0.1,
          "number": 2,
          "delay": 0,
          "x": 0
        }
      ],
