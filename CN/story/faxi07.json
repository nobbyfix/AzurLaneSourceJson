      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "这就是你所期望的吗，我的姐姐？",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_faxi2"
    },
    {
