      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "嗯……和我的预感一样。",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_107"
    },
    {
