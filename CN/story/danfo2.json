    {
      "mode": 1,
      "sequence": [
        [
          "丹佛的港区委托！\n\n<size=45>二 第一个任务，第一支团队</size>",
          1
        ]
      ],
      "stopbgm": true
    },
    {
      "actor": 102150,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "flashout": {
        "alpha": [
          0,
          1
        ],
        "dur": 1,
