        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_map_maliyana"
    },
    {
      "actor": 102140,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
