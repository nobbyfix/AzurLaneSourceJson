      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "actorName": "终端",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "star_level_bg_106"
