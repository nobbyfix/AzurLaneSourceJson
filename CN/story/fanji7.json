    {
      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "say": "事实上，这也是她的魅力之一。",
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_nepu1"
    },
    {
      "say": "呜……真是太狡猾了……",
      "actor": 204020,
      "side": 2,
