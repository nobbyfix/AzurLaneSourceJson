      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "{namecode:92}紧忙扶住了倒下{namecode:161}，焦急的呼喊着"
