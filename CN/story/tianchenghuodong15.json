      "side": 2,
      "dir": 1,
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "blackBg": true,
      "say": "「因为内心的软弱而犯下的错误，甚至牵连了前来救援的同伴」"
