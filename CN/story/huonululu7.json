      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "withoutActorName": true,
      "say": "说话间，火奴鲁鲁的脸红的就像她那头鲜艳的头发一样。",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
