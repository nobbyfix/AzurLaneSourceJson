      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_school"
    },
    {
      "say": "으음, 확실히... 만약 누군가 인디짱을 괴롭힌다면——",
      "actor": 103060,
      "side": 2,
      "dir": 1,
      "nameColor": "#a9f548",
